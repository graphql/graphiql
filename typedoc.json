{
  "name": "GraphQL Official LSP Documentation",
  "theme": "default",
  "mode": "modules",
  "out": "packages/graphiql/typedoc",
  "inputFiles": [
    "**/node_modules/vscode*/**",
    "**/node_modules/@types/node/**"
  ],
  "exclude": "**/__tests__/**,*.spec.*,**/*.stories.tsx,*-test.*,**/esm/**,**/dist/**,**/node_modules/**,packages/graphiql-2-rfc-*/**",
  "excludePrivate": true,
  "excludeNotExported": true,
  "external-modulemap": ".*/packages/([\\w\\-_]+)/",
  "ignoreCompilerErrors": true,
  "includeDeclarations": true,
  "preserveConstEnums": true,
  "readme": "./README.md"
}
