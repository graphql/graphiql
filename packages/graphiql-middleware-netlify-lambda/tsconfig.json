{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "lib": ["esnext", "dom"],
    "sourceMap": true,
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "declaration": true
  }
}
