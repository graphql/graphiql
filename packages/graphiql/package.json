{
  "name": "graphiql",
  "version": "0.16.0",
  "description": "An graphical interactive in-browser GraphQL IDE.",
  "contributors": [
    "Hyohyeon Jeong <asiandrummer@fb.com>",
    "Lee Byron <lee@leebyron.com> (http://leebyron.com/)"
  ],
  "repository": "http://github.com/graphql/graphiql",
  "homepage": "http://github.com/graphql/graphiql/tree/master/packages/graphiql#readme",
  "bugs": {
    "url": "https://github.com/graphql/graphiql/issues?q=issue+label:graphiql"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "esm",
    "bundle",
    "graphiql.min.js",
    "graphiql.css",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn run build-js && yarn run build-flow",
    "build-bundles": "bash ./resources/buildBundles.sh",
    "build-js": "bash ./resources/buildBabel.sh",
    "build-flow": "node ../../resources/buildFlow.js",
    "check": "flow check",
    "dev": "PORT=8080 babel-node test/server",
    "e2e": "kill -9 `cat test/pid` | true && yarn e2e:server && cypress run && kill -9 `cat test/pid`",
    "e2e:server": "PORT=8080 babel-node test/server.js & wait-on http://localhost:8080",
    "test": "node ../../resources/runTests ENZYME=true"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "codemirror": "^5.47.0",
    "codemirror-graphql": "^0.11.2",
    "copy-to-clipboard": "^3.2.0",
    "graphql": "14.5.6",
    "isomorphic-fetch": "^2.2.1",
    "markdown-it": "^8.4.0",
    "regenerator-runtime": "^0.13.3"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0",
    "prop-types": ">=15.5.0",
    "react": "^15.6.0 || ^16.0.0",
    "react-dom": "^15.6.0 || ^16.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.9",
    "acorn": "^7.1.0",
    "acorn-dynamic-import": "^4.0.0",
    "autoprefixer": "9.6.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "dynamic-cdn-webpack-plugin": "^4.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-15": "^1.4.0",
    "express": "5.0.0-alpha.5",
    "express-graphql": "0.9.0",
    "jest": "^24.8.0",
    "jsdom": "15.0.0",
    "module-to-cdn": "^3.1.5",
    "postcss-cli": "6.1.2",
    "prop-types": "15.7.2",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-test-renderer": "15.6.2",
    "serve": "^11.2.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  }
}
