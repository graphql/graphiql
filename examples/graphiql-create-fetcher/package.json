    "cross-env": "^7.0.2",
    "css-loader": "^3.5.1",
    "html-webpack-plugin": "^4.2.0",
    "react-dom": "^16.12.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "worker-loader": "^2.0.0"
